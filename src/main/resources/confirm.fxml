<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: lightsteelblue;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ATMapp.controllers.ConfirmController">
   <children>
      <Label alignment="CENTER" layoutX="136.0" layoutY="159.0" prefHeight="303.0" prefWidth="550.0" rotate="45.0" text="ATM" textFill="#2b804d72">
         <font>
            <Font name="System Italic" size="150.0" />
         </font>
         <effect>
            <InnerShadow color="#ddbaba" />
         </effect>
      </Label>
      <Label fx:id="confirmLabel" alignment="CENTER" layoutX="11.0" layoutY="196.0" prefHeight="58.0" prefWidth="777.0" text="confirm" AnchorPane.leftAnchor="11.0" AnchorPane.rightAnchor="12.0">
         <font>
            <Font name="System Bold Italic" size="35.0" />
         </font>
      </Label>
      <Button fx:id="yesCfBtn" layoutX="281.0" layoutY="316.0" mnemonicParsing="false" onAction="#handleYesCfBtn" style="-fx-background-color: darkseagreen; -fx-background-radius: 10;" text="YES">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="noCfBtn" layoutX="467.0" layoutY="316.0" mnemonicParsing="false" onAction="#handleNoBtn" style="-fx-background-color: indianred; -fx-background-radius: 10;" text="NO">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
